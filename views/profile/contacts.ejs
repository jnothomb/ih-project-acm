<!-- main.html -->
<!DOCTYPE html>
<html>

<head>
    <title>Contacts</title>
</head>

<body>

    <h1>Contacts</h1>
    <br>

    <% if(user) { %>

        <h3>List</h3>

        <ul>
            <% user.forEach(user => { %>
                <li>
                    <button type="button">
                        <%= user.firstName %>
                            <%= user.lastName %>
                    </button>
                </li>
                <% }) %>
        </ul>

        <a href="/profile/:<%= user.id %>"> profile...</a>
        <br>

        <% } %>

</body>

</html>